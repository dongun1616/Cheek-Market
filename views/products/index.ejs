<% layout('layouts/boilerplate') %>
    <h1>All Products</h1>
    <div>
        <a href="/products/new">Add Product</a>
    </div>

    <ul>
        <% for (let product of products ) {%>
            <li><a href="/products/<%= product._id%>">
                    <%= product.title%>
                </a>
            </li>
            <%}%>
    </ul>